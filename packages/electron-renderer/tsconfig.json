  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "allowJs": true,
    "declaration": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "outDir": "plugins",
    "strict": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["exclude", "src/build-config.vite-3.0.5.ts"]
}
